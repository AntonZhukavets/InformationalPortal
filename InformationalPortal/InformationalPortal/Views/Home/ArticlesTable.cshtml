<table border="1">
    @{
        int k = 0;
        for (int i = 0; i < 10; i++)
        {

            <tr>
                @for (int j = 0; j < 3; j++)
                {
                    k++;
                    <td><span id="span_@k.ToString()"></span></td>
                }
            </tr>

        }
    }
</table>
<div id="loadDiv"></div>